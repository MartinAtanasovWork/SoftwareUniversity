{
    "env": {
       "browser": true,
       "node": true,
       "mocha": true,
       "es2021":true
    },
    "globals": {},
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module",
        "ecmaFeatures": {}   
    },
    "rules": {
        "semi":"error",
        "curly":"error",
        "quotes": [
            "warn",
            "double"
        ],
        "no-undef":"error"
    }
}